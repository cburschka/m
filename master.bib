month = {aug},
number = {1},
pages = {42--65},
title = {{lsomorphism of Graphs of Bounded Valence Can Be Tested in Polynomial Time}},
url = {http://www.sciencedirect.com/science/article/pii/0022000082900095},
volume = {25},
