@inproceedings{Otto1997,
abstract = {We give a novel and simple characterization of first-order logic and its$\backslash$ninfinitary variants with bounded numbers of variables in terms of$\backslash$ncircuit computation. The main feature of the proposed circuit model of$\backslash$nexplicitly symmetric circuits is, that genericity in computations over$\backslash$nstructures is guaranteed through full combinatorial symmetry of circuits$\backslash$nwith respect to permutations of the input representation.$\backslash$nDefinability in the bounded variable fragment of infinitary logic is$\backslash$nequivalent with recognizability in such explicitly symmetric circuits in$\backslash$nwhich the orbits of nodes under symmetry operations are polynomially$\backslash$nbounded. First-order definability is equivalent with recognizability in$\backslash$nfinite depth circuits of that kind.},
author = {Otto, M},
booktitle = {Computer Science Logic},
isbn = {3-540-63172-0},
issn = {0302-9743},
keywords = {circuit complexity,finite model theory,generic computation},
pages = {369--384},
title = {{The logic of explicitly presentation-invariant circuits}},
volume = {1258},
year = {1997}
}
